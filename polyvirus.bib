	month = mar,
	pmcid = {PMC7093599},
	pmid = {32256466},
	title = {A {Single} {ssRNA} {Segment} {Encoding} {RdRp} {Is} {Sufficient} for {Replication}, {Infection}, and {Transmission} of {Ourmia}-{Like} {Virus} in {Fungi}},
	url = {https://www.ncbi.nlm.nih.gov/pmc/articles/PMC7093599/},
	urldate = {2021-03-24},
