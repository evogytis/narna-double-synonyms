	number = {11},
	pages = {e80720},
	title = {Novel Virus Discovery and Genome Reconstruction from Field {RNA} Samples Reveals Highly Divergent Viruses in Dipteran Hosts},
	url = {https://journals.plos.org/plosone/article?id=10.1371/journal.pone.0080720},
	volume = {8},
	year = {2013},
	Bdsk-Url-1 = {https://journals.plos.org/plosone/article?id=10.1371/journal.pone.0080720},
