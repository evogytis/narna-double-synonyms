@article{Chandler2015,
	abstract = {Mosquitoes, most often recognized for the microbial agents
	  of disease they may carry, harbor diverse microbial communities that include
	    viruses, bacteria, and fungi, collectively called the microbiota. The
	    composition of the microbiota can directly and indirectly affect disease
	    transmission through microbial interactions that could be revealed by its
	    characterization in natural populations of mosquitoes. Furthermore, the use of
	    shotgun metagenomic sequencing (SMS) approaches could allow the discovery of
	    unknown members of the microbiota. In this study, we use RNA SMS to
	    characterize the microbiota of seven individual mosquitoes (species include
		Culex pipiens, Culiseta incidens, and Ochlerotatus sierrensis) collected from a
	    variety of habitats in California, USA. Sequencing was performed on the
	    Illumina HiSeq platform and the resulting sequences were quality-checked and
	    assembled into contigs using the A5 pipeline. Sequences related to single
	    stranded RNA viruses of the Bunyaviridae and Rhabdoviridae were uncovered,
	  along with an unclassified genus of double-stranded RNA viruses. Phylogenetic
	    analysis finds that in all three cases, the closest relatives of the identified
	    viral sequences are other mosquito-associated viruses, suggesting widespread
	    host-group specificity among disparate viral taxa. Interestingly, we identified
	    a Narnavirus of fungi, also reported elsewhere in mosquitoes, that potentially
	    demonstrates a nested host-parasite association between virus, fungi, and
	    mosquito. Sequences related to 8 bacterial families and 13 fungal families were
	    found across the seven samples. Bacillus and Escherichia/Shigella were
	    identified in all samples and Wolbachia was identified in all Cx. pipiens
	    samples, while no single fungal genus was found in more than two samples. This
	    study exemplifies the utility of RNA SMS in the characterization of the natural
	    microbiota of mosquitoes and, in particular, the value of identifying all
	    microbes associated with a specific host.},
	author = {Chandler, James Angus and Liu, Rachel M. and Bennett, Shannon N.},
	date-modified = {2020-02-14 15:29:37 +0000},
	doi = {10.3389/fmicb.2015.00185},
	issn = {1664-302X},
	journal = {Front. Microbiol.},
	language = {English},
	title = {{RNA} shotgun metagenomic sequencing of northern {California} ({USA}) mosquitoes uncovers viruses, bacteria, and fungi},
	url = {https://www.frontiersin.org/articles/10.3389/fmicb.2015.00185/full},
	volume = {6},
	year = {2015},
	Bdsk-Url-1 = {https://www.frontiersin.org/articles/10.3389/fmicb.2015.00185/full},
	Bdsk-Url-2 = {http://dx.doi.org/10.3389/fmicb.2015.00185}}

@article{Charon2019,
	abstract = {{\textless}h3{\textgreater}Abstract{\textless}/h3{\textgreater} {\textless}p{\textgreater}Parasites of the genus \textit{Plasmodium} cause human malaria. Yet nothing is known about the viruses that infect these divergent eukaryotes. We investigated the \textit{Plasmodium} virome by performing a meta-transcriptomic analysis of blood samples from malaria patients infected with \textit{P. vivax, P. falciparum} or \textit{P. knowlesi}. This revealed a novel bi-segmented narna-like RNA virus restricted to \textit{P. vivax} and named Matryoshka RNA virus 1 (MaRNAV-1) to reflect its ``Russian doll'' nature: a virus, infecting a parasite, infecting an animal. MaRNAV-1 was abundant in geographically diverse \textit{P. vivax} from humans and mosquitoes. Notably, a related virus (MaRNAV-2) was identified in Australian birds infected with a \textit{Leucocytozoon} - eukaryotic parasites that group with \textit{Plasmodium} in the Apicomplexa subclass hematozoa. This is the first report of a \textit{Plasmodium} virus. As well as broadening our understanding of the eukaryotic virosphere, the restriction to \textit{P. vivax} may help understand \textit{P. vivax}-specific biology in humans and mosquitoes.{\textless}/p{\textgreater}},
